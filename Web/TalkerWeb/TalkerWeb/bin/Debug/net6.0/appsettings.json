{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PasswordValidationParameters": {
    "Regex": "^[a-zA-Z\\d\\\\\\[\\]_\\*\\-!]*$",
    "MinimumNumberOfCharacters": 8,
    "MaximumNumberOfCharacters": 24
  }
}
